{
  "name": "webglue.me-vue",
  "version": "v1.0.0-beta.1",
  "description": "webglue.me implemented with Vue",
  "author": "PAYW",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:express": "cross-env NODE_ENV=development nodemon ./src/server/index.js --watch ./src/",
    "dev:live": "nuxt dev --port=3100",
    "build": "nuxt build",
    "start": "nuxt start --port=3000",
    "start:express": "cross-env NODE_ENV=production node ./src/server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "canvas-confetti": "^0.4.2",
    "cross-env": "^6.0.3",
    "disable-scroll": "^0.5.0",
    "express": "^4.17.1",
    "frameguard": "^3.1.0",
    "nuxt": "^2.10.0",
    "vue-color": "^2.7.0"
  },
  "devDependencies": {
    "@nuxt/types": "^0.3.4",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "jest": "^24.1.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.4",
    "sass-loader": "^8.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^4.0.0-0"
  }
